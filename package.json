{
  "name": "api-service",
  "version": "3.0.0",
  "description": "Nimble",
  "author": "Tung Nguyen",
  "private": true,
  "license": "Internal Use License",
  "keywords": [
    "typescript",
    "nestjs",
    "Nimble",
    "api-svc"
  ],
  "scripts": {
    "db": "aws ssm start-session --target i-07d75bb57a42dfe78 --document-name AWS-StartPortForwardingSessionToRemoteHost --parameters host=\"api-service-qa-db.cadc39zh5hiz.us-west-2.rds.amazonaws.com\",portNumber=\"5432\",localPortNumber=\"5438\" --profile nimble",
    "redis": "aws ssm start-session --target i-07d75bb57a42dfe78 --document-name AWS-StartPortForwardingSessionToRemoteHost --parameters host=\"redis-nimble-ro.9rdqkx.ng.0001.usw2.cache.amazonaws.com\",portNumber=\"6379\",localPortNumber=\"6378\" --profile nimble",
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "dev": "nest start --watch",
    "start:dev": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --colors --verbose --detectOpenHandles",
    "test:unit": "yarn test --config ./jest.config-unit.js",
    "test:e2e": "yarn test --config ./jest.config-e2e.js",
    "test:ci": "yarn test --ci --coverage",
    "detect-secrets": "git diff --staged --name-only | xargs detect-secrets-hook --baseline .secrets.baseline --exclude-files '.*.npmrc$'",
    "detect-secrets:scan": "detect-secrets scan --baseline .secrets.baseline",
    "detect-secrets:audit": "detect-secrets audit .secrets.baseline",
    "prisma:m": "npx prisma migrate dev",
    "prisma:m-c": "npx prisma migrate dev --create-only",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:g": "npx prisma generate",
    "fix:husky": "chmod +x .husky/pre-commit && chmod +x bin/detect-secrets",
    "commit": "git-cz",
    "prepare": "husky",
    "init-be": "bash ./bash/init-be.sh"
  },
